.row
	.col-xs-12
		#ma(data-spy="affix",data-offset-top="85")
			nav.navbar.navbar-default
				.navbar-header
					a.navbar-brand(href="{{stream.self.href}}") {{stream.title}}
				.collapse.navbar-collapse
					ul.nav.navbar-nav.navbar-right
						li
							a(href='#',alt='Mark all as Read') 
								i.fa.fa-fw.fa-check
						li
							a(href='#',alt='Refresh') 
								i.fa.fa-fw.fa-refresh
						li
							a(href='#stream-info',alt='Options',data-toggle="collapse",data-parent="#map") 
								i.fa.fa-fw.fa-ellipsis-v
		.well.collapse#stream-info
			.row.list-group
				.col-xs-6.list-group-item
					.row-content
						h4.list-group-item-heading Site URL
						p.list-group-item-text 
							input(type="text",class="form-control",value='{{stream.alternate[0].href}}',disabled='')
							span.material-input
					.row-content
						h4.list-group-item-heading Description
						p.list-group-item-text {{stream.description}}
				.col-xs-6.list-group-item
					.row-content
						h4.list-group-item-heading Update time
						p.list-group-item-text {{stream.updated}}